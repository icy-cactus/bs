<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.FeedbackDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.FeedbackEntity" id="feedbackMap">
        <result property="picture" column="picture"/>
        <result property="title" column="title"/>
        <result property="feedbacktime" column="feedbacktime"/>
        <result property="content" column="content"/>
        <result property="userid" column="userid"/>
        <result property="username" column="username"/>
        <result property="tips" column="tips"/>
        <result property="sfsh" column="sfsh"/>
        <result property="shhf" column="shhf"/>
    </resultMap>


    <select id="selectListView"
		resultType="com.entity.view.FeedbackView" >

		SELECT  feedback.* FROM feedback  feedback
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.FeedbackView" >
		SELECT * FROM feedback  feedback <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
